{       
  "languageserver": {       
    "go": {       
      "command": "gopls",       
      "rootPatterns": ["go.mod"],       
      "trace.server": "verbose",       
      "filetypes": ["go"],       
      "options": {       
          "editor.action.organizeImport": "goimports"       
      }       
    }       
  }       
}
